{
    "parser": "babel-eslint",
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": 0,
        "max-len": [1, 120, 2, { "ignoreComments": true }],
        "prop-types": [2],
        "no-debugger": 0
    },
    "extends": [
        "eslint:standard",
        "plugin:react/recommended",
        "plugin:standard"
    ]
}
